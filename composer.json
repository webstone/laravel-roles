{
    "name": "7lab/laravel-roles",
    "description": "Powerful package for handling roles and permissions in Laravel.",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.8.*|^6.0|^7.0|^8.0",
        "illuminate/database": "5.8.*|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ultraware\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^3.9|^4.0|^5.0|^6.0",
        "orchestra/database": "^3.8|^4.3|^5.0|^6.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0",
        "satooshi/php-coveralls": "^2.0",
        "sebastian/environment": "^4.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ultraware\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
