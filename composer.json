{
    "name": "7lab/laravel-roles",
    "description": "Powerful package for handling roles and permissions in Laravel.",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0.2",
        "illuminate/support": "^8.12|^9.0|^10.0",
        "illuminate/database": "^8.12|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ultraware\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.23|^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^9.4",
        "sebastian/environment": "^5.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ultraware\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
